#include <iostream> 
#include <string> 
using namespace std; 
int main() 
{ 
  int a,b,c,d,e,f; 
  cout << "Enter the variable value \n"; 
  cin >> a >>b >>c; 
  d= a+b; 
  e= a+c; 
  f= b+c; 
  if (d>e) 
    if (d>f) 
      cout << "result is " <<d; 
    else  
      cout << "result is " <<f; 
  else  
    if (e>f) 
      cout << "result is " <<e; 
    else  
      cout << "result is " <<f;    
} 
