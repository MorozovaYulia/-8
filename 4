#include <iostream> 
#include <string> 
using namespace std; 
#include <math.h> 
int main() 

{ 
  int x, y; 
  cout << "Enter the X coordinate of point A \n" ; 
  cin >> x ; 
  cout << "Enter the Y coordinate of point A \n" ; 
  cin  >> y; 
  if (x>0) 
    if (y>0) 
      cout << "First quarter"; 
    else  
      cout << "Fourth quarter"; 
  else 
    if  (y>0) 
      cout << "Second quarter; 
    else  
      cout << "Third quarter"; 
} 
