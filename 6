#include <iostream> 
#include <string> 
#include <math.h> 
using namespace std;
int main()
{ 
  int a; 
  cout << "Enter the number \n" ; 
  cin >> a ; 
  if (a/10 <  10) 
    if (a%2==0) 
      cout << " it is a double even digit"; 
    else  
      cout << " it is a double odd digit"; 
  else  
    if (a%2==0) 
      cout << " it is a  three-digit even number"; 
    else  
      cout << " it is a  three-digit odd numbert"; 
} 
